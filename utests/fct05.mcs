int cmp(int a, int b) {
    if (a - b > 0) {
        return 1;
    } else {
        if (a - b < 0) {
            return -1;
        } else {
            return 0;
        }
    }
}

int max(int a, int b) {
    if (cmp(a, b) == 1) {
        return a;
    } else {
        return b;
    }
}

int main() {
    return max(9, 3);
}
